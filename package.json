{
  "name": "react-native-boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "bash -c 'source scripts/android/sdk/init.sh && react-native run-android'",
    "android:emulator": "bash -c 'source scripts/android/sdk/init.sh && scripts/android/android_emulator.sh'",
    "ios": "scripts/ios_init.sh && react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "mobx": "^6.6.1",
    "mobx-react": "^7.5.2",
    "react": "18.2.0",
    "react-native": "0.69.4"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/runtime": "^7.12.18",
    "@react-native-community/eslint-config": "^3.1.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^28.1.6",
    "@types/react-native": "^0.69.5",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "babel-jest": "^28.1.3",
    "eslint": "^8.21.0",
    "eslint-plugin-jest": "^26.8.2",
    "jest": "^28.1.3",
    "metro-react-native-babel-preset": "^0.72.0",
    "prettier": "^2.6.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.7.4",
    "@babel/plugin-proposal-decorators": "^7.0.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
